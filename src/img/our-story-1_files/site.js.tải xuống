$(document).on('keypress', '#cus-phone-num', function(e){
    //number only
    if(e.which >= 48 && e.which <= 57) {
        var length = $(this).val().length;
        //if not start with 0 and too much number
        if ((length == 0 && e.which !== 48) || length > 12) {
            return false;
        }
        return true;
    }
    return false;
})


function validationStep(step) {
    var result = true;
    switch(step) {
        case 1:
            if ($(".am-wrapper input[name=deliver-schedule]:checked").length == 0) {
                result = false;
                $(".label-deliver-schedule").addClass('text-danger');
                setTimeout(function() {$(".label-deliver-schedule").removeClass('text-danger');}, 4000);
            }

            // if ($(".am-wrapper input[name=deliver-time]:checked").length == 0) {
            //     result = false;
            //     $(".label-deliver-time").addClass('text-danger');
            //     setTimeout(function() {$(".label-deliver-time").removeClass('text-danger');}, 4000);
            // }

            if ($(".am-wrapper .choose-a-pack.active").length == 0) {
                result = false;
                $(".label-choose-a-pack").addClass('text-danger');
                setTimeout(function() {$(".label-choose-a-pack").removeClass('text-danger');}, 4000);
            }
            if (result) {
                subscription['deliver-pack'] = $(".am-wrapper .choose-a-pack.active").data('id');
                subscription['deliver-schedule'] = $(".am-wrapper input[name=deliver-schedule]:checked").val();
                subscription['deliver-time'] = $(".am-wrapper input[name=deliver-time]:checked").val();
            } else {
                $(".am-wrapper .service-errors p").text("Opps. Bạn vui lòng chọn đầy đủ thông tin!");
                $(".am-wrapper .service-errors").removeClass('hidden');
                setTimeout(function() {$(".am-wrapper .service-errors").addClass('hidden');}, 5000);
                return false;
            }
            break;
        case 2:
            var cusEmail = $('.am-wrapper #cus-email').val();
            if ( cusEmail && !validateEmail(cusEmail) ) {
                $('.am-wrapper #cus-email').addClass('input-required');
                $(".am-wrapper .order-errors p").text("Opps. Invalid email format!");
                $(".am-wrapper .order-errors").removeClass('hidden');
                return false;
            } else {
                $(".am-wrapper .order-errors").addClass('hidden');
            }

            var dob = $('.am-wrapper #dob').val();
            if ( dob && !validateBirthday(dob) ) {
                $('.am-wrapper #dob').addClass('input-required');
                $(".am-wrapper .order-errors p").text("Opps. Birthday format must be mm/dd/YYYY");
                $(".am-wrapper .order-errors").removeClass('hidden');
                return false;
            } else {
                $(".am-wrapper .order-errors").addClass('hidden');
            }

            $(".am-wrapper .purchase-steps.step-2 input.required[type=text]").each(function(){
                if ($(this).val().trim() == "") {
                    result = false;
                    $(this).addClass('input-required');
                } else {
                    $(this).removeClass('input-required');
                    subscription[$(this).attr('name')] = $(this).val().trim();
                }
            })
            $(".am-wrapper .purchase-steps.step-2 textarea").each(function(){
                subscription[$(this).attr('name')] = $(this).val().trim();
            })

            if ($(".am-wrapper .purchase-steps.step-2 select").val().trim() == "") {
                result = false;
                $(".am-wrapper .purchase-steps.step-2 select").addClass('input-required');
                // setTimeout(function() {$(".am-wrapper .purchase-steps.step-2 select").removeClass('input-required');}, 4000);
            } else {
                $(".am-wrapper .purchase-steps.step-2 select").removeClass('input-required');
                subscription['cus-district'] = $(".am-wrapper .purchase-steps.step-2 select").val().trim();
            }

            let voucher = $(".am-wrapper .purchase-steps.step-2 input[name='voucher']").val().trim();
            subscription['voucher'] = voucher;

            // if (result) {
            //     $("#confirm-order-modal").modal('show');
            // }

            if (result && submitState == false) {
                $.ajax({
                    url: '/site/submit-order',
                    cache: false,
                    data: subscription,
                    type: 'post',
                    beforeSend: function(){
                        $(".am-wrapper #confirm-order").button('loading');
                        submitState = true;
                    },
                    success: function(data){
                        submitState = false;
                        if (data.status) {
                            window.location.href = "/order-success?t=jd";
                            // $('.am-wrapper .order-complete').html(data.html);
                            // $('.am-wrapper .order-success').trigger('click');
                        } else {
                            $(".am-wrapper #confirm-order").button('reset');
                            $(".am-wrapper .order-errors p").text(data.error);
                            $(".am-wrapper .order-errors").removeClass('hidden');
                            if(data.name == 'voucher'){
                                $(".am-wrapper .purchase-steps.step-2 input[name='voucher']").addClass('input-required');
                            } else {
                                $(".am-wrapper .purchase-steps.step-2 input[name='voucher']").removeClass('input-required');
                            }
                            // setTimeout(function() {$(".am-wrapper .order-errors").addClass('hidden');}, 5000);
                        }
                    }
             });
            }
            break;
        default:
            // result = false;
    }
    return result;
}

function validateEmail(email) {
    var re = /^(([^<>()\[\]\\.,;:\s@"]+(\.[^<>()\[\]\\.,;:\s@"]+)*)|(".+"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
    return re.test(email);
}

function validateBirthday(birthday) {
    var re = /^(0[1-9]|[1-2][0-9]|3[0-1])\/(0[1-9]|1[0-2])\/[0-9]{4}$/;
    return re.test(birthday);
}

function registerJuiceCleanse() {
    var input = '';
    var required = false;

    var cusEmail = $('#cus-email').val();
    if ( cusEmail && !validateEmail(cusEmail) ) {
        $('.am-wrapper #cus-email').addClass('alert-danger');
        $(".error-message").html('Opps. Invalid email format!');
        $(".jc-errors-div").removeClass('hide');
        return false;
    } else {
        $(".error-message").html('');
    }

    var cusDob = $('#dob').val();
    if ( cusDob && !validateBirthday(cusDob) ) {
        $('.am-wrapper #dob').addClass('alert-danger');
        $(".error-message").html('Opps. Birthday format must be mm/dd/YYYY');
        $(".jc-errors-div").removeClass('hide');
        return false;
    } else {
        $(".error-message").html('');
    }

    $(".jc-register .required").each(function(){
        if (!$(this).val()) {
            $(this).addClass('alert-danger');
            required = true;
        } else {
            $(this).removeClass('alert-danger');
        }
    }) 

    input = $(".jc-register input[type!=radio],textarea").serialize();
    if (required){
        $(".error-message").html('Không được để trống các trường bắt buộc');
        $(".jc-errors-div").removeClass('hide');
        return false;
    }

    if (!$('.jc-register input[type=radio]:checked').length) {
        $('.jc-program-text').addClass('text-danger');
        $(this).focus();
        /*setTimeout(function(){
            $('.jc-program-text').removeClass('text-danger');
        }, 4000)*/
        return false;
        
    } else {
        input += '&jc-program=' + $('input[type=radio]:checked').val();
    }

    if (!$('.jc-register select[name=cus-district]').val()) {
        $('select[name=cus-district]').addClass('alert-danger');
        $(this).focus();
        /*setTimeout(function(){
            $('select[name=cus-district]').removeClass('alert-danger');
        }, 4000)*/
    } else {
        input += '&cus-district=' + $(".jc-register select[name=cus-district]").val();
    }
    input += '&jc=1'

    $.ajax({
            url: '/site/submit-order',
            cache: false,
            data: input,
            type: 'post',
            beforeSend: function(){
                $("#submit-jc").button('loading');
                submitState = true;
            },
            success: function(data){
                submitState = false;
                if (data.status) {
                    window.location.href = "/order-success?t=jc";
                } else {
                    $("#submit-jc").button('reset');
                    $(".error-message").html(data.error);
                    $(".jc-errors-div").removeClass('hide');

                    if(data.name == 'voucher'){
                        $("input[name='voucher']").addClass('alert-danger');
                    } else {
                        $('select[name=voucher]').removeClass('alert-danger');
                    }
                   /* setTimeout(function(){
                        $(".error-message").html('');
                        $(".jc-errors-div").addClass('hide');
                    }, 4000)*/
                }
            }
     });
}

function registerGreenJuice() {
    var input = '';
    var invalid = false;

    var cusEmail = $('#cus-email').val();
    var dob = $('#dob').val();
    if ( cusEmail && !validateEmail(cusEmail) ) {
        $(this).addClass('alert-danger');
        alert('Invalid email format.');
        return false;
    }
    if ( dob && !validateBirthday(dob) ) {
        $(this).addClass('alert-danger');
        alert('Birthday format must be mm/dd/YYYY');
        return false;
    }

    $(".jc-register .required").each(function(){
        if (!$(this).val()) {
            $(this).addClass('alert-danger');
           /* setTimeout(function(){
                $("input.alert-danger").removeClass('alert-danger');
            }, 4000)*/
            invalid = true;
        } else {
            $(this).removeClass('alert-danger');
        }
    });

    if ($('.jc-register input[name="cus-pack"]').length) {
        if (!$('.jc-register input[name="cus-pack"]:checked').val()) {
            invalid = true;
        }
    }

    if (invalid) {
        $(".error-message").html('Không được để trống các trường bắt buộc');
        $(".jc-errors-div").removeClass('hide');
        return false;
    }

    input = $(".jc-register input,textarea,select").serialize()

    $.ajax({
            url: '/site/submit-order',
            cache: false,
            data: input,
            type: 'post',
            beforeSend: function(){
                $("#submit-gj").button('loading');
                submitState = true;
            },
            success: function(data){
                submitState = false;
                if (data.status) {
                    window.location.href = "/order-success?t=jg";
                } else {
                    $("#submit-gj").button('reset');
                    $(".error-message").html(data.error);
                    $(".jc-errors-div").removeClass('hide');

                    if(data.name == 'voucher'){
                        $("input[name='voucher']").addClass('alert-danger');
                    } else {
                        $('select[name=voucher]').removeClass('alert-danger');
                    }

                    /*setTimeout(function(){
                        $(".error-message").html('');
                        $(".jc-errors-div").addClass('hide');
                    }, 4000)*/
                }
            }
     });
}

function preShowJCModal(e) {
    var relatedTarget = e.relatedTarget;
    
    $(".jc-register input[type!=radio],textarea").each(function(){
        $(this).val('');//clean previous data set
    }) 
    $('select[name=cus-district]').val('');
    $('.jc-register input[type=radio]:checked').prop('checked',false);
}
